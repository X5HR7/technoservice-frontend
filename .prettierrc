{
  "arrowParens": "avoid",
  "jsxSingleQuote": true,
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@App/(.*)$",
    "^@pages/(.*)$",
    "^@modules/(.*)$",
    "^@ui/(.*)$",
    "^@providers/(.*)$",
    "^@constants/(.*)$",
    "^@utils/(.*)$",
    "^@assets/(.*)$",
    "^@store/(.*)$",
    "^@hooks/(.*)$",
    "^@api/(.*)$",
    "^[../]",
    "^[./]",
    "(.scss)$"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
